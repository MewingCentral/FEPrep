{
  "name": "@feprep/nextjs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "test": "pnpm with-env vitest run",
    "test:watch": "vitest run --watch",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.6",
    "@feprep/api": "workspace:*",
    "@feprep/auth": "workspace:*",
    "@feprep/consts": "workspace:*",
    "@feprep/db": "workspace:*",
    "@feprep/transactional": "workspace:*",
    "@feprep/ui": "workspace:*",
    "@feprep/validators": "workspace:*",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.18.1",
    "@tanstack/react-table": "^8.13.2",
    "@trpc/client": "11.0.0-next-beta.264",
    "@trpc/react-query": "11.0.0-next-beta.264",
    "@trpc/server": "11.0.0-next-beta.264",
    "@uploadthing/react": "^6.4.1",
    "framer-motion": "^11.0.24",
    "geist": "^1.2.2",
    "input-otp": "^1.2.3",
    "next": "^14.1.0",
    "oslo": "^1.1.3",
    "pdf-merger-js": "^5.1.1",
    "pdfjs-dist": "^4.0.379",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-pdf": "^7.7.1",
    "superjson": "2.2.1",
    "uploadthing": "^6.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@feprep/eslint-config": "workspace:*",
    "@feprep/prettier-config": "workspace:*",
    "@feprep/tailwind-config": "workspace:*",
    "@feprep/tsconfig": "workspace:*",
    "@testing-library/react": "^14.3.0",
    "@types/node": "^20.11.13",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "jiti": "^1.21.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vitest": "^1.4.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@feprep/eslint-config/base",
      "@feprep/eslint-config/nextjs",
      "@feprep/eslint-config/react"
    ]
  },
  "prettier": "@feprep/prettier-config"
}
